import math
a = float(intput('podaj współczynnik a'))
b = float(intput('podaj współczynnik b'))
c = float(intput('podaj współczynnik c'))

if b == 0 and c == 0:
   print('x = 0')
elif b == 0 and c != 0:
    if -c/a > 0:
       print('x1 = {}'.format(math.sqrt(-c/a)))
       print('x2 = {}'.format(-math.sqrt(-c/a)))
    elif -c/a < 0:
       print('sprzeczne')
elif c == 0 and b != 0:
    print('x1 = {}'.format(0))
    print('x2 = {}'.format(-b/a))
